{
  "name": "react-native-fbsdk-next",
  "version": "6.2.0",
  "description": "Facebook SDK support for React Native apps.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Marcos BÃ©rgamo"
  },
  "contributors": [
